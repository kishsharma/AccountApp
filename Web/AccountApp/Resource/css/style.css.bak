/*Added by Chetan because of button is redirecting in the accordion so i change the button to span*/
/* $$$$$ START $$$$ */
.expand_collapse_box span.accordion {
    background-image: url(../images/plus.png);
    background-repeat: no-repeat;
    background-position: right 6px;
    background-color: #fff;
    border: 1px solid #ccc;
    color: #444;
    cursor: pointer;
    float: left;
    font-family: arial;
    font-size: 14px;
    margin: 0 0 5px;
    outline: medium none;
    padding: 8px 10px;
    text-align: left;
    transition: all 0.4s ease 0s;
    width: 98%;
}
.expand_collapse_box span.accordion.active, span.accordion:hover {
	background-image:url(../images/minus.png);
    background-color: #00bcd5;
    color: #fff;
	border:1px solid #007c95;
}
/* $$$$$ END $$$$ */
.expand_collapse_box button.accordion {
	background-image:url(../images/plus.png);
	background-repeat:no-repeat;
	background-position:right 6px;
    background-color: #fff;
    border: 1px solid #ccc;
    color: #444;
    cursor: pointer;
    float: left;
    font-family: arial;
    font-size: 14px;
    margin: 0 0 5px;
    outline: medium none;
    padding: 8px 10px;
    text-align: left;
    transition: all 0.4s ease 0s;
    width: 100%;
}
.expand_collapse_box button.accordion:hover {
	background-image:url(../images/plus.png);
}
.expand_collapse_box button.accordion.active, button.accordion:hover {
	background-image:url(../images/minus.png);
    background-color: #00bcd5;
    color: #fff;
	border:1px solid #007c95;
}
.expand_collapse_box div.panel {
    background-color: white;
    display: none;
    float: left;
    font-family: arial;
    font-size: 12px;
    line-height: normal;
    margin: 0 0 20px;
    padding:8px 14px;
    width: 97%;
}
.expand_collapse_box div.panel.show {
    display: block;
}
