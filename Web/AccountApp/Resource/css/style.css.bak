    width: 99%;
}
.crsl-items {
    margin: 0 auto;
    width: 89% !important;
}
.panel {
    background-color: #ffffff;
    box-shadow: 0 0 1px #cccccc;
    float: left;
    margin: 5px;
    overflow: hidden;
    padding: 3px;
    width: 180px;
}
.panel_img {
    float: left;
    height: auto;
    padding: 0;
    position: relative;
    text-align: center;
    width: auto;
}
.panel_img {
    text-align: center;
}
.stepcarousel_images #navbtns a{
	margin-top:100px;
}
.stepcarousel_images .panel .enlarge_box{
    background-color: rgba(0, 0, 0, 0.5);
    display: none;
    float: left;
    height: auto;
    left: 0;
    margin:100px auto 0;
    max-width: 620px;
    padding: 40px 0;
    position: fixed;
    right: 0;
    text-align: center;
    top: 0;
    z-index: 500000;
}
.stepcarousel_images .panel:hover .enlarge_box{
	display:block;
}
.stepcarousel_images .panel .enlarge_box img{
    max-height: 730px;
    max-width: 500px;
}
 .client_box {
    float: left;
    width: 100%;
    height: auto;
}
.client_box ul li.client_list {
    background-color: #00bcd5;
    border: 3px solid #fff;
    border-radius: 34px;
    box-shadow: 1px 0 1px #ccc;
    color: #fff;
    float: left;
    text-decoration:none;
    height: auto;
    margin: 0 20px 20px 0;
    padding: 0;
    text-align: center;
    width: 20%;
    position: relative;
}

.client_box ul li a.client_heading1 {
    float: left;
    width: 100%;
    text-decoration:none;
    height: auto;
    font-family: arial;
    font-size: 16px;
    color: #fff;
    
}
.client_box ul li a.client_remove{
    background-color: rgba(0, 0, 0, 0.5);
    border-radius: 34px;
    color: #fff;
    display: none;
    float: left;
    text-decoration:none;
    font-family: arial;
    font-size: 29px;
    font-weight: bold;
    height: 100%;
    line-height: 170%;
    position: absolute;
    text-align: center;
    width: 100%;
}
.client_box ul li.client_list:hover .client_remove{
	display:block;
}
.dataTables_length{
    float: left;
    font-family: arial;
    font-size: 15px;
    height: auto;
    line-height: 23px;
    width: auto;
}
.dataTables_length label{
    color: #024792;
    float: left;
    font-weight: bold;
    height: auto;
    width: auto;
}
.dataTables_length select{
    border: 1px solid #ccc;
    float: left;
    height: auto;
    margin: 0 10px;
    padding: 3px;
    width: auto;
	font-family:Arial, Helvetica, sans-serif;
	font-size:14px;
}
.dataTables_length input{
    border: 1px solid #ccc;
    float: left;
    height: auto;
    margin: 0 10px;
    padding: 3px;
    width: auto;
	font-family:Arial, Helvetica, sans-serif;
	font-size:14px;
}
.dataTables_filter{
	float:right;
}
#DataList{
    background-color: #fff;
    border: 1px solid #ccc;
    color: #000;
    float: left;
    font-family: arial;
    font-size: 14px;
    height: auto;
    width: 100%;
	margin:0 0 20px 0;
}
#DataList tr{
    border-bottom: 1px solid #ccc;
    float: left;
    height: auto;
    width: 100%;
}
.purchase_list_box{
    border-bottom: 1px solid #ccc;
    float: left;
    height: auto;
    width: 100%;
}
#DataList tr th{
    border-right: 1px solid #ccc;
    float: left;background-color: #e0e0e0;
    height: auto;
    padding: 11px 0;
    text-indent: 20px;
    width: 33.2%;
}
#DataList tr td{
    border-right: 1px solid #ccc;
    float: left;
    height: auto;
    padding: 11px 0;
    text-indent: 20px;
    width: 33.2%;
}
#DataList tr td a{
	color:#00bcd5;
	text-decoration:none;
}
.total_box {
	float:left;
	width:100%;
	height:auto;
	margin:20px 0px;
}
.total_list{
    float: left;
    height: auto;
    margin: 0 0 20px;
    width: 100%;
}
.total_box input{
    background-color: #fff;
    border: 1px solid #ccc;
    border-radius: 3px;
    color: #000;
    float: left;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 12px;
    font-weight: normal;
    height: auto;
    margin: 0 0 0 10px;
    padding: 10px 0;
    text-indent: 10px;
    width: 16%;
}
.total_label{
    float: left;
    font-family: arial;
    font-size: 12px;
    font-weight: bold;
    height: auto;
    margin: 10px 0 0;
    width: 42%;
}

/*-----------FORGOT-PASSWORD-START----------*/
.forgotpass_open{
	height:0px;
	overflow:hidden;
}
.forgotpass {
    background-color: #f2f2f2;
    border: 0 none;
    height: 200px;
    overflow: hidden;
    padding: 0;
    position: relative;
    width: 100%;
    z-index: 1002;
	transition-duration:0.5s;
}
.forgotpass2{
	height:0px;
	transition-duration:0.5s;
	overflow:hidden;
}
/*-----------FORGOT-PASSWORD-END----------*/
.forgot_pass_heading{
    float: left;
    font-family: arial;
    font-size: 27px;
    font-weight: bold;
    padding: 20px 0;
    text-align: center;
    text-transform: uppercase;
    width: 100%;
}
#forgotpass2{
	text-align:center;
}
#forgotpass2 input{
    background-color: #606060;
    border: 0 none;
    border-radius: 2px;
    clear: both;
    color: #fff;
    font-family: arial;
    font-size: 16px;
    margin: 0 auto;
    padding: 10px 0;
    text-align: center;
    width: 88%;
	cursor:pointer;
}
#forgotpass2 input[type="submit"]{
    background-color: #00bcd5;
    margin-top: 20px;
    padding: 8px 20px;
    text-decoration: none;
    text-transform: uppercase;
    width: auto;
}
.close_btn{
    background-color: #000;
    border-radius: 30px;
    color: #fff;
    float: right;
    font-family: arial;
    font-size: 12px;
    font-weight: bold;
    line-height: normal;
    margin: 7px 5px 0 0;
    padding: 5px 8px;
    position: absolute;
    right: 0;
    text-decoration: none;
}
.forgotpass_open .close_btn{
	display:none;
}
.forgotpass2 .close_btn{
	display:none;
}
.purchase_itom_box{
    border-bottom: 5px solid #fff;
    border-top: 5px solid #fff;
    float: left;
    height: auto;
    width: 100%;
}
}
@media screen and (max-width: 1024px) {
.middle_content{
    float: none;
    margin: 0 auto;
    width: 97%;
}
.left_col{
	width:100%;
	min-height:inherit;
	margin-bottom:20px;
}
.left_menu ul li{
	width:40%;
}
.right_column{
	width:100%;
	padding:0px 0 20px 0;
}
.box_list{
	width:48%;
}
.company_box{
	width:99%;
}
.company_menu li{
	width:33%;
}
.company_menu li a{
    border-bottom: 1px solid #000;
    margin: 0 0 1px;
}
.company_list{
	width:45%;
}

#A {
	width:100%;
}
#B{
    left: 0 !important;
    width: 100%;
	position:relative;
}
.hide_show{
	display:none;
}
.left_col{
	position:relative;
}
}

@media screen and (max-width: 800px) {
/*MENU-DESIGN*/
.menuopen_text{
	background-image:url(../images/menu_icon.png);
	background-repeat:no-repeat;
	background-position:right center;
