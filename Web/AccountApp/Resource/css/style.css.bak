/*ZOOP-IMAGE-START*/
.info_image p{
    background-color: rgba(0, 0, 0, 0.4);
    color: #fff;
    font-family: arial;
    padding: 10px 0;
    position: absolute;
    text-align: center;
    bottom: 0;
	margin:0px;
    width: 100%;
}
.zoom {
	display:inline-block;
	position: relative;
}
.zoom:after {
	content:'';
	display:block; 
	width:33px; 
	height:33px; 
	position:absolute; 
	top:0;
	right:0;
	background:url(icon.png);
}
.zoom .zoomImg {
	display: none;
}
.zoom:hover zoomImg {
	display: block;
}
.zoom img::selection { background-color: transparent; }

#ex2 img:hover { cursor: url(grab.cur), default; }
#ex2 img:active { cursor: url(grabbed.cur), default; }

/*ZOOP-IMAGE-END*/

